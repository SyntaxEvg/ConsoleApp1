{
  "FileSettings": {
    "ExtensionMapping": {
      "HTML": [ ".htm", ".html" ],
      "JSON": [ ".json" ],
      "JavaScript": [ ".js" ]
    },
    "PriorityGroups": [ "HTML", "JSON", "JavaScript"],
    "IsReplacements": true,
    "Replacements": [
      {
        "OldStr": " (RU)",
        "NewStr": ""
      },
      {
        "OldStr": "Application Port",
        "NewStr": "88888888888888888888888"
