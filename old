  // Apply filters and echo
  return apply_filters( 'bizup_wpex_get_excerpt', $output );
}


//Demo content file include here

function bizup_import_files() {
  return array(
    array(
      'import_file_name'           => 'Bizup Demo Import',
      'categories'                 => array( 'Category 1' ),
      'import_file_url'            => trailingslashit( get_template_directory_uri() ) . 'ocdi/bizup-content.xml',
      'import_widget_file_url'     => trailingslashit( get_template_directory_uri() ) . 'ocdi/bizup-widget.wie',
      'import_redux'               => array(
        array(
          'file_url'    => trailingslashit( get_template_directory_uri() ) . 'ocdi/bizup-options.json',
          'option_name' => 'bizup_option',
        ),
      ),

      'import_notice'              => esc_html__( 'Caution: For importing demo data please click on "Import Demo Data" button. During demo data installation please do not refresh the page.', 'bizup' ),

    ),

  );
}
