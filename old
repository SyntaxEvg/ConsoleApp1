// Способ 1: Ручной подсчет через цикл for...in
var dict = {
    "ключ1": "значение1",
    "ключ2": "значение2",
    "age": 30
};

var count = 0;
for (var key in dict) {
    count++;
}
print("Количество элементов: " + count); // 3

// Способ 2: Функция подсчета
function getSize(obj) {
    var size = 0;
    for (var key in obj) {
        size++;
    }
    return size;
}

var size = getSize(dict);
print(size); // 3

function isEmpty(obj) {
    for (var key in obj) {
        return false; // Если хоть один ключ есть - не пустой
    }
    return true;
}

var dict = {};
if (isEmpty(dict)) {
    print("Словарь пустой");
}

===========
var translations = {
    "ru": "Анализ",
    "en": "Analysis",
    "es": "Análisis"
};

// Подсчет количества языков
var langCount = 0;
for (var lang in translations) {
    langCount++;
}
print("Доступно языков: " + langCount);

// Получение всех ключей в массив
var languages = [];
for (var lang in translations) {
    languages.push(lang);
}
print("Языки: " + languages.join(", ")); // "ru, en, es"

// Функция получения перевода с проверкой
function getTranslation(lang) {
    if (translations[lang]) {
        return translations[lang];
    }
    return translations["en"] || "N/A";
}

print(getTranslation("ru")); // "Анализ"
