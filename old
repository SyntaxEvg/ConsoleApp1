/var/www/wp-content/plugins/elementor/modules/home/transformations/

// Проверяем существование ключа 'get_started'
	if ( ! isset( $home_screen_data['get_started'] ) || ! is_array( $home_screen_data['get_started'] ) ) {
		$home_screen_data['get_started'] = [];
	}
public function transform( array $home_screen_data ): array {
		$new_get_started = [];

		foreach ( $home_screen_data['get_started'] as $index => $item ) {
			if ( $this->is_valid_item( $item ) ) {
				$new_get_started[] = $item;
			}
		}

		$home_screen_data['get_started'] = reset( $new_get_started );

		return $home_screen_data;
	}
