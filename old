$ClientId = "a3071ce5-2875-4541-90fb-53c1a5e46aaf"
Write-Host ""
$ServerApp = Get-AdfsServerApplication | Where-Object { $_.Identifier -eq $ClientId }
if ($ServerApp) {
    Write-Host "Server Application: $($ServerApp.Name)" -ForegroundColor Green
    Write-Host "Текущий Redirect URI:" -ForegroundColor Yellow
    Write-Host "  $($ServerApp.RedirectUri)" -ForegroundColor White
    Write-Host ""
}
