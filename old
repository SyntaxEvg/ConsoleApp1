sudo ps aux | grep "php-fpm: pool www" | head -3
sudo cat /etc/php-fpm.d/www.conf | grep -v "^;" | grep -v "^$"
sudo cat /etc/php-fpm.d/www.conf | grep -iE "security|chroot|chdir"
sudo -u nginx php -r "file_put_contents('/var/www/wp-content/uploads/elementor/css/test.css', 'test');" && echo "PHP OK" || echo "PHP FAIL"
